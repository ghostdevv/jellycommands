    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "tsm": "^2.3.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.0",
    "uvu": "^0.5.6"
  },
  "peerDependencies": {
    "discord.js": "^14.0.0"
  },
